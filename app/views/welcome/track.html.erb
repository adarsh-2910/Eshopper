<div class="chose_area">
	<%= form_with model: @orders, url: welcome_track_path, method: :post,local: true,data: { turbo: false } do |form| %>
        <div class="mb-3">
            <%= form.label :user_order_id %>
            <%= form.text_field :user_order_id %>
        </div>
        <div class="mb-3">
            <%= form.submit "Submit", class: "btn btn-primary" %>
        </div>
	<% end %>
</div>
<div>status:<%= @user_order.present? ? @user_order.status : @error %></div>
